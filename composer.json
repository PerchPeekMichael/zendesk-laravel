{
    "name": "PerchPeekMichael/zendesk-laravel",
    "type": "library",
    "description": "Laravel wrapper for zendesk/zendesk_api_client_php package with new Guzzle Psr7 call",
    "keywords": ["zendesk"],
    "homepage": "https://perchpeek.com",
    "license": "MIT",
    "authors": [
        {
            "name": "PerchPeek Michael",
            "email": "michael.guimaraes@perchpeek.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.4|^6.0|^7.0|^8.0",
        "PerchPeekMichael/zendesk_api_client_php": "2.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huddle\\Zendesk\\Providers\\ZendeskServiceProvider"
            ],
            "aliases": {
                "Zendesk": "Huddle\\Zendesk\\Facades\\Zendesk"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Huddle\\Zendesk\\": "src"
        }
    },
    "minimum-stability": "stable"
}
